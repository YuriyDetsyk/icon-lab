{
  "name": "bd-server",
  "version": "2.1.0",
  "description": "Icon Lab Web API",
  "main": "index.js",
  "author": "Chunkup",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "webpack",
    "watch": "nodemon",
    "start": "node -r ts-node/register/transpile-only ./src/index.ts",
    "clean": "rimraf node_modules dist",
    "test:watch": "jest --watchAll",
    "test:single": "jest --coverage=true",
    "lint": "eslint",
    "migrate": "sequelize db:migrate",
    "migrate:seed": "sequelize db:seed:all",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed",
    "seed:undo": "sequelize db:seed:undo",
    "seed:undo:all": "sequelize db:seed:undo:all"
  },
  "engines": {
    "npm": ">=8.6.0",
    "node": ">=18.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest-junit": {
    "outputDirectory": "./coverage"
  },
  "dependencies": {
    "aws-sdk": "2.1212.0",
    "bcrypt": "5.0.1",
    "connect-session-sequelize": "7.1.5",
    "cors": "2.8.5",
    "express": "4.18.1",
    "express-session": "1.17.3",
    "image-to-base64": "2.2.0",
    "jimp-watermark": "1.1.0",
    "jsonwebtoken": "8.5.1",
    "multiparty": "4.2.3",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4",
    "reflect-metadata": "0.1.13",
    "require-main-filename": "2.0.0",
    "sequelize": "6.21.6",
    "svg2img": "1.0.0-beta.2",
    "tmp": "0.2.1",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.5",
    "@types/image-to-base64": "2.1.0",
    "@types/jest": "26.0.15",
    "@types/jsonwebtoken": "8.5.9",
    "@types/multiparty": "0.0.33",
    "@types/node": "18.7.16",
    "@types/tmp": "0.2.3",
    "@types/uuid": "8.3.4",
    "@types/validator": "13.7.6",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "clean-webpack-plugin": "3.0.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "jest-junit": "12.0.0",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sequelize-cli": "6.2.0",
    "ts-jest": "26.4.3",
    "ts-loader": "9.3.1",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "2.5.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  }
}
