{
  "name": "bd-server",
  "version": "1.0.0",
  "description": "Icon Lab Web API",
  "main": "index.js",
  "author": "Chunkup",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "webpack",
    "watch": "nodemon",
    "start": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all && node -r ts-node/register src/index.ts",
    "clean": "rimraf node_modules dist",
    "test:watch": "jest --watchAll",
    "test:single": "jest --coverage=true",
    "lint": "eslint",
    "sequelize": "sequelize",
    "gen:migration": "sequelize migration:generate",
    "gen:model": "sequelize model:generate",
    "gen:seed": "sequelize seed:generate",
    "migrate:init": "sequelize init:migrations",
    "migrate": "sequelize db:migrate",
    "migrate:down": "sequelize db:migrate:undo",
    "migrate:down:all": "sequelize db:migrate:undo:all",
    "migrate:seed": "sequelize db:seed:all",
    "migrate:seed:down": "sequelize db:seed:undo",
    "migrate:seed:down:all": "sequelize db:seed:undo:all"
  },
  "engines": {
    "node": "12.16.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest-junit": {
    "outputDirectory": "./coverage"
  },
  "dependencies": {
    "aws-sdk": "^2.814.0",
    "bcrypt": "^5.0.1",
    "connect-session-sequelize": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "image-to-base64": "^2.2.0",
    "jimp-watermark": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "multiparty": "^4.2.2",
    "pg": "^8.5.1",
    "pg-connection-string": "^2.4.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "sequelize-typescript": "^2.0.0",
    "svg2img": "^0.9.2",
    "tmp": "^0.2.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.8",
    "@types/dotenv-flow": "^3.1.0",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.3",
    "@types/image-to-base64": "^2.1.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multiparty": "0.0.32",
    "@types/node": "^14.14.22",
    "@types/tmp": "^0.2.0",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-junit": "^12.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0",
    "webpack-node-externals": "^2.5.2"
  }
}
